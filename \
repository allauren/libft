#include "libft.h"

int main(int argc, char *argv[])
{
	/* test ft_memset */
	if(argc>=2)
	{
	ft_putstr(ft_memset("argv[1]", '-', 6));
	ft_putstr(ft_memset("argv[1]", 'i', 10));
	}
	return 0;
}
